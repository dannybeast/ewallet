  +b.SECTION.form-section.section
    .container
      h3.title Остались вопросы? задайте их нам!
      form
        +e.block(class="--gradient")
          +e.row
            +b.field-row
              +e.LABEL.name Ваше имя:
              +b.INPUT(type="text" class="field")
            +b.field-row
              +e.LABEL.name E-mail:
              +b.INPUT(type="email" class="field --error")
            +b.field-row
              +e.LABEL.name Телефон:
              +b.INPUT(type="text" class="field")
          +e.row
            +b.field-row(class="--textarea")
              +b.TEXTAREA(class="field") Текст вопроса
          +e.row
            // btn--disabled
            +b.BUTTON.btn(class="--gr-gray") Задать вопрос
  //
  +b.bottom-skew
    +e.decor
    +b.SECTION.form-section.section
        .container
          form
            +e.block
              +e.H3.title.gr-accent Вступайте в LuxLife Club!
              +e.desc
                p Просто заполните эту форму и наши консультанты перезвонят Вам!
              +e.row
                +b.field-row
                  +e.LABEL.name Ваше имя:
                  +b.INPUT(type="text" class="field --white")
                +b.field-row
                  +e.LABEL.name E-mail:
                  +b.INPUT(type="email" class="field --white --error")
                +b.field-row
                  +e.LABEL.name Телефон:
                  +b.INPUT(type="text" class="field --white")
                // btn--disabled
                +b.BUTTON.btn(class="--gr-accent") Отправить
    +footer()