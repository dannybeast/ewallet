    +b.SECTION.lk-section
      //
      +b.lk-title-module
        +e.H3.title Наши продукты
    

    mixin product-item(image, title, price)
      +b.product-item
        +e.image
          img(src=image, alt="")
        +e.content
          +e.P.title  #{title}
          if block
            block
        +e.bottom
          +e.A(href="#", class="btn") Записаться на курс
          +e.P.price #{price}



    +b.SECTION.lk-section
      //
      +b.products
        +e.filters
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Фото/видео
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Инвестор
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Бизнес
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Маркетинг
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Разработчик
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Дизайнер
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Продажи
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span Ораторское искусство
          +b.LABEL.filter-tag
            input(type="checkbox", name="product-filters")
            span ЗОЖ
        +e.list
          +e.col
