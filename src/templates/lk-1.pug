---
title: Личный кабинет
---

extends layouts/layout-lk

block variables 
  - var pageName = "lk-1"
  
block content

    +b.SECTION.lk-section
        //
        include partials/_lk-info
    
    +b.SECTION.lk-section
        //
        +b.lk-banners
            +e.row
                +e.col
                    // .active
                    +b.banner-module(class="--dark active")
                        +e.content
                            +e.P.title Бэк-офис
                            +e.P.desc <span>Окончание::</span> <big>22/09/19</big>
                            +e.buttons
                                +b.A(href="#", class="btn --gr-accent disabled") Активирован
                        +e.icon
                            svg(viewBox='0 0 512 512' xmlns='http://www.w3.org/2000/svg')
